{
  "name": "react-console-notify",
  "version": "0.1.14",
  "description": "A small React package that shows real-time notifications for console logs (log, warn, error, etc.) during development",
  "author": "Wexop",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "homepage": "https://wexop.github.io/react-console-notify",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wexop/react-console-notify.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --dts --format esm,cjs --external react,react-dom",
    "dev:example": "vite --config vite.config.ts",
    "build:example": "vite build --config example/vite.config.ts && rmdir /s /q docs && xcopy example\\\\dist docs /e /i && type nul > docs\\\\.nojekyll",
    "deploy": "gh-pages -d example/dist"
  },
  "peerDependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "gh-pages": "^6.3.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.6"
  },
  "keywords": [
    "log",
    "notify",
    "console",
    "react",
    "notification",
    "debug"
  ]
}
